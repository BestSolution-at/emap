package sample;

entity Country derived BaseObject {
	etype "http://www.bestsolution.at/sample/1.0"#Country
	
	attributes {
		primarykey id 	=> CO_ID generatedby { "h2" autokey },
		name			=> CO_NAME		
	}
	queries {
		single selectById(long id) {
			default Country FROM "COUNTRY" WHERE "CO_ID = ${id}"
		}
	}
}